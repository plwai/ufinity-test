{
  "extends": "airbnb-base",
  "plugins": ["prettier"],
  "rules": {
    "implicit-arrow-linebreak": "off",
    "no-unused-vars": ["error", { "argsIgnorePattern": "^_" }],
    "arrow-parens": [2, "as-needed"],
    "operator-linebreak": ["error", "after"],
    "comma-dangle": ["error", "only-multiline", { "functions": "never" }]
  }
}
